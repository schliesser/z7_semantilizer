{
	"files": [
		"./Resources/Private/JavaScript/Backend/*.ts"
	],
	"include": [
		"./Resources/Private/JavaScript/Backend/*.ts"
	],
	"exclude": [
		"node_modules",
		"./Resources/Private/JavaScript/Backend/TYPO3/**/*.ts"
	],
	"compilerOptions": {
		"target": "es2017",
		"allowSyntheticDefaultImports": true,
		"incremental": true,
		"tsBuildInfoFile": "./Resources/Private/JavaScript/Backend/tsconfig.tsbuildinfo",
		"module": "es2020",
		"moduleResolution": "node",
		"sourceMap": false,
		"removeComments": false,
		"alwaysStrict": true,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"useDefineForClassFields": false,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"pretty": true,
		"baseUrl": "./Resources/Private/JavaScript/Backend/",
		"paths": {
			"@zeroseven/semantilizer/*": [
				"*"
			],
			"@typo3/backend/*": [
				"./TYPO3/backend/*"
			],
			"@typo3/belog/*": [
				"./TYPO3/belog/*"
			],
			"@typo3/beuser/*": [
				"./TYPO3/beuser/*"
			],
			"@typo3/core/*": [
				"./TYPO3/core/*"
			],
			"@typo3/dashboard/*": [
				"./TYPO3/dashboard/*"
			],
			"@typo3/extensionmanager/*": [
				"./TYPO3/extensionmanager/*"
			],
			"@typo3/filelist/*": [
				"./TYPO3/filelist/*"
			],
			"@typo3/form/*": [
				"./TYPO3/form/*"
			],
			"@typo3/impexp/*": [
				"./TYPO3/impexp/*"
			],
			"@typo3/info/*": [
				"./TYPO3/info/*"
			],
			"@typo3/install/*": [
				"./TYPO3/install/*"
			],
			"@typo3/linkvalidator/*": [
				"./TYPO3/linkvalidator/*"
			],
			"@typo3/lowlevel/*": [
				"./TYPO3/lowlevel/*"
			],
			"@typo3/opendocs/*": [
				"./TYPO3/opendocs/*"
			],
			"@typo3/recycler/*": [
				"./TYPO3/recycler/*"
			],
			"@typo3/redirects/*": [
				"./TYPO3/redirects/*"
			],
			"@typo3/rte-ckeditor/*": [
				"./TYPO3/rte_ckeditor/*"
			],
			"@typo3/scheduler/*": [
				"./TYPO3/scheduler/*"
			],
			"@typo3/setup/*": [
				"./TYPO3/setup/*"
			],
			"@typo3/t3editor/*": [
				"./TYPO3/t3editor/*"
			],
			"@typo3/tstemplate/*": [
				"./TYPO3/tstemplate/*"
			],
			"@typo3/viewpage/*": [
				"./TYPO3/viewpage/*"
			],
			"@typo3/workspaces/*": [
				"./TYPO3/workspaces/*"
			]
		}
	}
}

