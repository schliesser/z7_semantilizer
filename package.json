{
  "name": "z7_semantilizer",
  "private": true,
  "config": {
    "typo3_repository": "https://github.com/TYPO3/typo3.git",
    "typescript_folder": "Build/Sources/TypeScript",
    "typescript_target": "./Resources/Private/JavaScript/Backend/Lib/TYPO3/"
  },
  "scripts": {
    "build": "gulp build",
    "build-dev": "GULP_CONTEXT=development gulp build",
    "test": "git clone --no-checkout $npm_package_config_typo3_repository $npm_package_config_typescript_target && cd $npm_package_config_typescript_target && git sparse-checkout set $npm_package_config_typescript_folder && git reset --hard HEAD && mv $npm_package_config_typescript_folder/* ."
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^17.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "lit": "^2.7.2",
    "sass": "^1.49.9",
    "tsify": "^5.0.4",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
